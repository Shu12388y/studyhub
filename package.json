{
  "name": "studyhub",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prepare": "husky",
    "write": "pnpm exec prettier . --write",
    "check": "pnpm exec prettier . --check"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.2",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "husky": "^9.1.7"
  }
}
